{
  "name": "zju-mirror",
  "private": false,
  "description": "ZJU open source mirror site",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve -H 0.0.0.0 -p 9000",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "extract": "yarn run babel --config-file ./babel-extract.config.js -o tmp/chunk.js 'src/**/*.{js,jsx,ts,tsx}' && rm -rf tmp",
    "types-check": "tsc --noEmit --project ./tsconfig.json",
    "lint-check": "eslint \"src/**/*\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/poppins": "^5.2.5",
    "@formatjs/intl-displaynames": "^5.4.3",
    "@iconify/icons-file-icons": "^1.2.3",
    "@iconify/icons-logos": "^1.2.36",
    "@iconify/icons-mdi": "^1.2.48",
    "@iconify/icons-simple-icons": "^1.2.74",
    "@iconify/react": "^5.2.0",
    "@mdx-js/mdx": "^3.1.0",
    "@mdx-js/react": "^2.3.0",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@svgr/webpack": "^8.1.0",
    "fuse.js": "^7.1.0",
    "gatsby": "^5.14.1",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-mdx": "^5.14.0",
    "gatsby-plugin-react-helmet": "^6.14.0",
    "gatsby-plugin-react-i18next": "^3.0.1",
    "gatsby-plugin-sass": "^6.14.0",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-plugin-svgr": "^3.0.0-beta.0",
    "gatsby-source-filesystem": "^5.14.0",
    "i18next": "^22.5.1",
    "js-search": "^2.0.1",
    "natsort": "^2.0.3",
    "path-to-regexp": "^6.3.0",
    "prism-react-renderer": "^2.4.1",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^12.3.1",
    "remark-gfm": "^1.0.0",
    "sass": "^1.86.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.0",
    "@babel/plugin-transform-typescript": "^7.27.0",
    "@eslint/js": "^9.23.0",
    "@types/js-search": "^1.4.4",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20.17.28",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.5",
    "babel-plugin-i18next-extract": "^0.8.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.10.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "http-proxy-middleware": "^3.0.3",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.3.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "webpack": "^5.99.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ZJUSCT/mirror-front"
  },
  "bugs": {
    "url": "https://github.com/ZJUSCT/mirror-front/issues"
  }
}
